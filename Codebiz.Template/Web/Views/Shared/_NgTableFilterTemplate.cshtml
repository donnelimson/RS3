<script type="text/ng-template" id="custom-ng-filters/clear-complaiantTableParams-filter-button.html">
    <button class="btn btn-default" ng-click="complaiantTableParams.filter({})">
        <span class="glyphicon glyphicon-refresh"></span> Clear
    </button>
</script>
